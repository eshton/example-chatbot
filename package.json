{
  "name": "my-facebook-chatbot",
  "version": "1.0.0",
  "description": "Created using Facebook Chatbot Template by Bonomi (http://bonomi.io)",
  "engines": {
    "node": "10.x"
  },
  "main": "index.js",
  "heroku-run-build-script": true,
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=dev ts-node --files app/index",
    "dev:watch": "NODE_ENV=dev nodemon --ignore *.test.* -e ts,tsx,js,ejs --exec ts-node --files src/index"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.3",
    "@types/node-fetch": "^2.5.2",
    "@types/request-promise-native": "^1.0.16",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.7",
    "nodemon": "^1.19.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/bonomi-io/facebook-chatbot-template"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
